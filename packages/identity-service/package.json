{
  "name": "@monorepo/identity-service",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "yarn clean",
    "build": "tsc --project tsconfig.app.json --sourceMap && rm -rf ./dist/migrations/*.d.ts ./dist/migrations/*.js.map",
    "build:watch": "tsc --project tsconfig.app.json --watch",
    "prestart": "yarn build",
    "start": "node --inspect=0.0.0.0:9229 ./dist/index.js"
  },
  "dependencies": {
    "@monorepo/core": "1.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "node-jose": "^2.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "18.15.0",
    "@types/node-jose": "^1.1.10",
    "@types/uuid": "^9.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
