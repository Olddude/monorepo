{
  "name": "@monorepo/identity-service",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "yarn clean",
    "build": "tsc --project tsconfig.app.json --sourceMap && rm -rf ./dist/migrations/*.d.ts ./dist/migrations/*.js.map",
    "build:watch": "tsc --project tsconfig.app.json --watch",
    "prestart": "yarn build",
    "start": "node --inspect=0.0.0.0:9229 ./dist/index.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "log4js": "^6.9.1",
    "node-jose": "^2.2.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "pg": "^8.11.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/knex": "^0.16.1",
    "@types/log4js": "^2.3.5",
    "@types/node": "18.15.0",
    "@types/node-jose": "^1.1.10",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "^0.3.9",
    "@types/uuid": "^9.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
